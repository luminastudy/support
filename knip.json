{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": ["src/**/*.{ts,tsx,js,jsx}"],
  "project": ["**/*.{ts,tsx,js,jsx}"],
  "ignore": [
    "build/**",
    ".docusaurus/**",
    "coverage/**",
    "*.config.js",
    "*.config.mjs",
    "scripts/**"
  ],
  "ignoreDependencies": [
    "@docusaurus/plugin-content-docs",
    "@lumina-study/logo"
  ],
  "ignoreExportsUsedInFile": true,
  "ignoreWorkspaces": []
}
